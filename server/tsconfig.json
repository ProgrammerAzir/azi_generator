{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2016",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true
  },
  
  "include": ["../vite.config.ts"],
  "module": "esnext",
  "moduleResolution": "node",
  "esModuleInterop": true,
  "ts-node": {
      "esm": true,
      "experimentalSpecifierResolution": "node"
    }
}
